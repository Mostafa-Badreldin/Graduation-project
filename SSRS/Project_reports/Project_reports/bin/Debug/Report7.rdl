          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="choice">
          <DataField>choice</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Exam">
