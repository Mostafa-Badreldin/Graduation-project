  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>examination_system</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Exam_ID">
            <Value>=Parameters!Exam_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        exam_Q_std.Q_ID, Questions.Question, Question_choices.choice
FROM            exam_Q_std INNER JOIN
                         Questions ON exam_Q_std.Q_ID = Questions.Q_id INNER JOIN
                         Question_choices ON Questions.Q_id = Question_choices.Q_id
WHERE        (exam_Q_std.exam_id = @Exam_ID)</CommandText>
      </Query>
      <Fields>
        <Field Name="Q_ID">
          <DataField>Q_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Question">
