  <DataSets>
    <DataSet Name="Grades">
      <Query>
        <DataSourceName>examination_system</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@std_id">
            <Value>=Parameters!std_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Student.std_id, Student.std_fname, Student.std_lname, Course.crs_name, student_course.grade_overall
FROM            Student INNER JOIN
                         student_course ON Student.std_id = student_course.std_id INNER JOIN
                         Course ON student_course.crs_id = Course.crs_id
WHERE        (Student.std_id = @std_id)</CommandText>
      </Query>
      <Fields>
        <Field Name="std_id">
          <DataField>std_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="std_fname">
          <DataField>std_fname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="std_lname">
          <DataField>std_lname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="crs_name">
          <DataField>crs_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="grade_overall">
          <DataField>grade_overall</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Student">
      <Query>
        <DataSourceName>examination_system</DataSourceName>
        <CommandText>SELECT        std_id, CONCAT(std_fname, ' ', std_lname)  AS 'Student name'
FROM            Student</CommandText>
      </Query>
      <Fields>
        <Field Name="std_id">
          <DataField>std_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Student_name">
          <DataField>Student name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
