  <DataSets>
    <DataSet Name="Course">
      <Query>
        <DataSourceName>examination_system</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Ins_ID">
            <Value>=Parameters!Ins_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Instructor.Ins_id, Course.crs_name, COUNT(student_course.std_id) AS 'Students number'
FROM            Instructor INNER JOIN
                         Instructor_Course ON Instructor.Ins_id = Instructor_Course.ins_id INNER JOIN
                         Course ON Instructor_Course.crs_id = Course.crs_id INNER JOIN
                         student_course ON Course.crs_id = student_course.crs_id
WHERE        (Instructor.Ins_id = @Ins_ID)
GROUP BY Instructor.Ins_id, Course.crs_name</CommandText>
      </Query>
      <Fields>
        <Field Name="Ins_id">
          <DataField>Ins_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="crs_name">
          <DataField>crs_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Students_number">
          <DataField>Students number</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Instructor">
